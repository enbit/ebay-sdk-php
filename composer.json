{
    "name": "enbit/ebay-sdk-php",
    "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
    "keywords": ["ebay", "api", "sdk", "PHP"],
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "enbit GmbH",
            "email": "info@enbit.de"
        }
    ],
    "support": {
        "issues": "https://github.com/enbit/ebay-sdk-php/issues"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "guzzlehttp/psr7": "^1.6 || ^2.0",
        "guzzlehttp/promises": "^1.3",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Enbit\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Enbit\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}
